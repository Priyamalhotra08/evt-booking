<!--Map iframe starts here-->
<div class="contact-map appear "  data-aos="fade-in" data-aos-delay="300" style=" pointer-events: none; margin-top: 50px;">
  <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d448193.95105503476!2d76.76355916510953!3d28.644287347703283!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x390cfd5b347eb62d%3A0x37205b715389640!2sDelhi!5e0!3m2!1sen!2sin!4v1746678479979!5m2!1sen!2sin"  allowfullscreen="" class="mt-5" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
</div>


<!--Map iframe end here-->
<div class="container contactpage">
	<div class="row">
		<!-- #content start here -->

		<div id="content" class="col-sm-12 appear" data-aos="fade-up" data-aos-delay="300">
      
			<form class="form-horizontal" method="post" action="/contact" >
				<fieldset>
				  <legend>Contact Us</legend>
          <% if (typeof message !== 'undefined') { %>
            <div class="d-flex justify-content-center mt-4">
              <div class="alert alert-<%= messageType %> text-center w-75" role="alert">
                <%= message %>
              </div>
            </div>
          <% } %>
					<div class="form-group required row">
						<div class="col-sm-6 mb-3">
              <input type="text" class="form-control" id="input-name" placeholder="Name" name="name" value="<%= typeof formData !== 'undefined' ? formData.name : '' %>" required>
						</div>
						<div class="col-sm-6 mb-3">
						  <input type="text" class="form-control" placeholder="E-Mail" id="input-email" value="<%= typeof formData !== 'undefined' ? formData.email : '' %>" name="email" required="">
						</div>
					</div>
					<div class="form-group required row">
						<div class="col-sm-6 mb-3">
						  <input type="text" class="form-control" id="input-website" placeholder="state" value="<%= typeof formData !== 'undefined' ? formData.state : '' %>" name="state" required="">
						</div>
						<div class="col-sm-6 mb-3">
						  <input type="text" class="form-control" placeholder="Phone Number" id="input-telephone" value="<%= typeof formData !== 'undefined' ? formData.telephone : '' %>" name="telephone" required="">
						</div>
					</div>
					<div class="form-group required row">
						<div class="col-sm-12 mb-3">
              <textarea class="form-control" placeholder="Please write your enquiry..." id="input-enquiry" rows="10" name="enquiry" required><%= typeof formData !== 'undefined' ? formData.enquiry : '' %></textarea>
						</div>
					</div>
					<div class="buttons">
						<div class="float-end ">
							<input type="submit" value="Submit" class="btn btn-info ps-5 pe-5 p-2 ">
						</div>
					</div>
				</fieldset>
			</form>	
    
			<div class="row">
				<div class="mainbox col-sm-12">	
					<h1 class="text-info fs-4 fw-bolder">Get in touch with us</h1>
					<div class="row">
						<div class="col-sm-4 appear"  data-aos="fade-right" data-delay="300">
							<div class="box1">
								<div class="icons">
								   <i aria-hidden="true" class="fa fa-phone"></i>
								</div><br/>
								<h3>Phone No </h3>
								<ul class="list-unstyled">
									<li>+91 9356372998</li>
									<li>+91 161-5012104</li>
								</ul>
							</div>
						</div>
						<div class="col-sm-4 appear"  data-aos="fade-up" data-delay="300">
							<div class="box1">
								<div class="icons">
								   <i aria-hidden="true" class="fa fa-envelope"></i>
								</div><br/>
								<h3>Email </h3>
								<ul class="list-unstyled">
									<li>info@themultimediadesigner.com </li>
									<li>info@realestate.com</li>
								</ul>
							</div>
						</div>
						<div class="col-sm-4 appear" data-aos="fade-left" data-delay="300" >
							<div class="box1">
								<div class="icons">
								   <i aria-hidden="true" class="fa fa-map-marker-alt"></i>
								</div><br/>
								<h3>Address</h3>
								<ul class="list-unstyled">
									<li>shop No:120, MD complex, <br>Noida New Delhi-141003 Delhi, INDIA	</li>
								</ul>
							</div>
						</div>
					</div>
				</div>				
			</div>				
		</div>
		<!-- #content end here -->
	</div>
</div>
<!-- main container end here -->
